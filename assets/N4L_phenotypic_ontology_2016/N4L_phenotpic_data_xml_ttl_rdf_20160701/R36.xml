<?xml version="1.0" encoding="UTF-8"?>
<entities  xmlns="http://namesforlife.com/ns/interchange" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://namesforlife.com/ns/interchange assertions.xsd" xmlns:identifier="http://namesforlife.com/ns/identifier">       
    <!-- # R(36) Organism that grows optimally in microaerobic condition and does not grow in anoxic and air conditions has microaerophilic phenotype.#  -->
    <entity doi="10.1601/ex.0000">
        <assertion>
                <observational-data>
                    <observation ontology-class="http://doi.org/10.1601/OptimalGrowth"/>
                    <condition condition-class="http://doi.org/10.1601/Microaerobic"/>                
                </observational-data>
        </assertion>
        
        <assertion>
            <observational-data>
                <observation ontology-class="http://doi.org/10.1601/NoGrowth"/>
                <condition condition-class="http://doi.org/10.1601/Anoxic"/>                
            </observational-data>
        </assertion>
 
        <assertion>
            <observational-data>
                <observation ontology-class="http://doi.org/10.1601/NoGrowth"/>
                <condition condition-class="http://doi.org/10.1601/Air"/>                
            </observational-data>
        </assertion>        
    </entity>
    
</entities>