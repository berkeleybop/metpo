    rdfs:range xsd:string .

d3o:hasWidth a owl:DatatypeProperty ;
    rdfs:domain d3o:CellSize ;
    rdfs:range xsd:string .

d3o:hasWidthRangeStart a owl:DatatypeProperty ;
    rdfs:domain d3o:CellSize ;
    rdfs:range xsd:float .

d3o:hasWidthRangeEnd a owl:DatatypeProperty ;
    rdfs:domain d3o:CellSize ;
    rdfs:range xsd:float .

d3o:hasWidthUnit a owl:DatatypeProperty ;
