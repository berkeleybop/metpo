<?xml version="1.0" encoding="UTF-8"?>
<entities  xmlns="http://namesforlife.com/ns/interchange" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://namesforlife.com/ns/interchange assertions.xsd" xmlns:identifier="http://namesforlife.com/ns/identifier">       
    <!-- # R(55) If an organism grows in air conditions and organism has Aerotolerant anaerobe phenotype, then the organism grow in microaerobic conditions.#-->
    <entity doi="10.1601/ex.0000">
            
        <assertion>
                <observational-data>
                    <observation ontology-class="http://doi.org/10.1601/Growth"/>
                    <condition condition-class="http://doi.org/10.1601/Air"/>                
                </observational-data>
        </assertion>
        
        <assertion>
            <phenotype ontology-class="http://doi.org/10.1601/AerotolerantAnaerobe"/>
        </assertion>
        
    </entity>
</entities>