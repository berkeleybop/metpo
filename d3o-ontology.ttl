    rdfs:range xsd:string .

# Salt tolerance properties
d3o:hasSaltInformation a owl:DatatypeProperty ;
    rdfs:domain d3o:SaltTolerance ;
    rdfs:range [ a rdfs:Datatype ;
        owl:unionOf ( xsd:string xsd:float rdf:langString ) ] .

d3o:hasSaltType a owl:DatatypeProperty ;
    rdfs:domain d3o:SaltTolerance ;
