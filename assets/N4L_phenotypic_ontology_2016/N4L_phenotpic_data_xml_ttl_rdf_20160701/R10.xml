<?xml version="1.0" encoding="UTF-8"?>
<entities  xmlns="http://namesforlife.com/ns/interchange" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://namesforlife.com/ns/interchange assertions.xsd" xmlns:identifier="http://namesforlife.com/ns/identifier">       
    <!-- # R(10) If an organism grows sub-optimally in moderate-high temperature conditions and optimally grows in mid-range temperature conditions, then the organism is Thermotolerant.#-->
    <entity doi="10.1601/ex.0000">
            
        <assertion>
                <observational-data>
                    <observation ontology-class="http://doi.org/10.1601/SubOptimalGrowth"/>
                    <condition condition-class="http://doi.org/10.1601/ModerateHighTemperatureCondition"/>                
                </observational-data>
        </assertion>
        
        <assertion>
            <observational-data>
                <observation ontology-class="http://doi.org/10.1601/OptimalGrowth"/>
                <condition condition-class="http://doi.org/10.1601/MidRangeTemperatureCondition"/>                
            </observational-data>
        </assertion>
        
    </entity>
</entities>